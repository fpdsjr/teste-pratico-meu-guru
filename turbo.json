{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    }
  },
  "lint-staged": {
    "apps/**/*.{ts, tsx}":[
      "eslint --fix"
    ]
  }
}